<h1>New Application</h1>

<%= form_with url: "/applications/create", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street_adress %>
  <%= f.text_field :street_adress %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.label :pitch %>
  <%= f.text_field :pitch%>

  <%= f.submit %>
<% end %>
